msgid "Fonts"
msgstr "Czcionki"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "Motyw"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "Pulpit"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
msgstr "Okna"

