msgid "Fonts"
msgstr "Tipi di carattere"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "Tema"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "Scrivania"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
msgstr "Finestre"

