#: ../data/gnome-tweak-tool.desktop.in.h:1 ../data/shell.ui.h:1
msgid "Tweak Tool"
msgstr "Алат за лицкање"

#: ../data/gnome-tweak-tool.desktop.in.h:2
msgid "Tweak advanced GNOME 3 settings"
msgstr "Налицкајте напредна подешавања Гнома 3"

#: ../data/shell.ui.h:2
msgid "welcome"
msgstr "Добродошли"

#: ../data/shell.ui.h:3
msgid "tweaks"
msgstr "Лицкања"

#: ../gtweak/tweakmodel.py:26
msgid "Fonts"
msgstr "Словни ликови"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "Тема"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "Радна површ"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
msgstr "Прозори"

#: ../gtweak/tweakmodel.py:30
msgid "Shell"
msgstr "Шкољка"

#. translate this the same as the name of the file manager (nautilus)
#: ../gtweak/tweakmodel.py:33
msgid "Files"
msgstr "Датотеке"

#: ../gtweak/tweakmodel.py:44
msgid "Miscellaneous"
msgstr "Остало"

#: ../gtweak/tweaks/tweak_interface.py:33
msgid "GTK+ theme"
msgstr "Гтк+ тема"

#: ../gtweak/tweaks/tweak_interface.py:51
msgid "Icon theme"
msgstr "Тема иконица"

#: ../gtweak/tweaks/tweak_interface.py:86 ../gtweak/tweaks/tweak_shell.py:117
msgid "<i>Default</i>"
msgstr "<i>Подразумевано</i>"

#: ../gtweak/tweaks/tweak_interface.py:87
msgid "Keybinding theme"
msgstr "Комбинација тастера"

#: ../gtweak/tweaks/tweak_shell_extensions.py:36
msgid "Extension downloading"
msgstr "Преузимање проширења"

#: ../gtweak/tweaks/tweak_shell_extensions.py:38
msgid "Error loading extension"
msgstr "Не могу да учитам проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:40
msgid "Extension does not support shell version"
msgstr "Проширење не подржава ово издање шкољке"

#: ../gtweak/tweaks/tweak_shell_extensions.py:42
msgid "Unknown extension error"
msgstr "Непозната грешка проширења"

#: ../gtweak/tweaks/tweak_shell_extensions.py:73
msgid "The shell must be restarted for changes to take effect"
msgstr "Морате поново да покренете шкољку како би применили измене"

#: ../gtweak/tweaks/tweak_shell_extensions.py:74
#: ../gtweak/tweaks/tweak_shell_extensions.py:143
msgid "Restart"
msgstr "Поново покрени"

#: ../gtweak/tweaks/tweak_shell_extensions.py:80
msgid "Install Shell Extension"
msgstr "Инсталирај проширење шкољке"

#: ../gtweak/tweaks/tweak_shell_extensions.py:84
msgid "Select an extension"
msgstr "Изаберите проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:89
#| msgid "Select an extension"
msgid "Get more extensions"
msgstr "Добавите још проширења"

#: ../gtweak/tweaks/tweak_shell_extensions.py:137
#, python-format
msgid "%s extension updated successfully"
msgstr "%s проширење је успешно ажурирано"

#: ../gtweak/tweaks/tweak_shell_extensions.py:139
#, python-format
msgid "%s extension installed successfully"
msgstr "%s проширење је успешно инсталирано"

#: ../gtweak/tweaks/tweak_shell_extensions.py:147
msgid "Error installing extension"
msgstr "Не могу да инсталирам проширење"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell_extensions.py:152
msgid "Invalid extension"
msgstr "Неисправно проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:160
msgid "Shell Extensions"
msgstr "Проширења шкољке"

#. check the shell is running and the usertheme extension is present
#: ../gtweak/tweaks/tweak_shell.py:58
msgid "Unknown error"
msgstr "Непозната грешка"

#: ../gtweak/tweaks/tweak_shell.py:63
msgid "Shell not running"
msgstr "Шкољка није покренута"

#: ../gtweak/tweaks/tweak_shell.py:84
msgid "Shell user-theme extension incorrectly installed"
msgstr "Проширење корисничке теме шкољке није исправно инсталирано"

#: ../gtweak/tweaks/tweak_shell.py:87
msgid "Shell user-theme extension not enabled"
msgstr "Проширење корисничке теме шкољке није омогућено"

#: ../gtweak/tweaks/tweak_shell.py:90
msgid "Could not list shell extensions"
msgstr "Не могу да испишем проширења шкољке"

#. a filechooser to install new themes
#: ../gtweak/tweaks/tweak_shell.py:122
msgid "Select a theme"
msgstr "Изаберите тему"

#: ../gtweak/tweaks/tweak_shell.py:171
#, python-format
msgid "%s theme updated successfully"
msgstr "%s тема је успешно ажурирана"

#: ../gtweak/tweaks/tweak_shell.py:173
#, python-format
msgid "%s theme installed successfully"
msgstr "%s тема је успешно инсталирана"

#: ../gtweak/tweaks/tweak_shell.py:181
msgid "Error installing theme"
msgstr "Не могу да инсталирам тему"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell.py:186
msgid "Invalid theme"
msgstr "Неисправна тема"

#: ../gtweak/tweaks/tweak_windows.py:31
msgid "Window theme"
msgstr "Тема прозора"

#. indicates the default theme, e.g Adwaita (default)
#: ../gtweak/utils.py:67
#, python-format
msgid "%s <i>(default)</i>"
msgstr "%s <i>(основно)</i>"

#: ../gtweak/widgets.py:305
msgid "Enable dark theme for all applications"
msgstr "Укључите тамну тему за све програме"

#: ../gtweak/widgets.py:306
msgid "Enable the dark theme hint for all the applications in the session"
msgstr "Укључите савет тамне теме за све програме у сесији"

#: ../gtweak/widgets.py:324
msgid "Error writing setting"
msgstr "Грешка записивања подешавања"

#~ msgid "Advanced Settings"
#~ msgstr "Напредна подешавања"

#~ msgid "%s Extension"
#~ msgstr "Проширење „%s“"
