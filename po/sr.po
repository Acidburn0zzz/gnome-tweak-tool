#: ../data/shell.ui.h:2
msgid "tweaks"
msgstr "Штимовања"

#: ../data/shell.ui.h:3
msgid "welcome"
msgstr "Добродошли"

#: ../data/gnome-tweak-tool.desktop.in.h:2
msgid "Tweak advanced GNOME 3 settings"
msgstr "Штимовање подешавања за Гном 3"

#: ../gtweak/tweaks/tweak_interface.py:33
msgid "GTK+ theme"
msgstr "Гтк+ тема"

#: ../gtweak/tweaks/tweak_interface.py:51
msgid "Icon theme"
msgstr "Тема иконица"

#: ../gtweak/tweaks/tweak_interface.py:86 ../gtweak/tweaks/tweak_shell.py:110
msgid "<i>Default</i>"
msgstr "<i>Подразумевано</i>"

#: ../gtweak/tweaks/tweak_interface.py:87
msgid "Keybinding theme"
msgstr "Комбинација тастера"

#. check the shell is running and the usertheme extension is present
#: ../gtweak/tweaks/tweak_shell.py:56
msgid "Unknown error"
msgstr "Непозната грешка"

#: ../gtweak/tweaks/tweak_shell.py:61
msgid "Shell not running"
msgstr "Шкољка није покренута"

#: ../gtweak/tweaks/tweak_shell.py:78
msgid "Shell user-theme extension incorrectly installed"
msgstr "Проширење за корисничке теме није инсталирано"

#: ../gtweak/tweaks/tweak_shell.py:81
msgid "Shell user-theme extension not enabled"
msgstr "Проширење за корисничке теме није омогућено"

#: ../gtweak/tweaks/tweak_shell.py:84
msgid "Could not list shell extensions"
msgstr "Не могу да испишем проширења шкољке"

#. a filechooser to install new themes
#: ../gtweak/tweaks/tweak_shell.py:115
msgid "Select a theme"
msgstr "Изаберите тему"

#: ../gtweak/tweaks/tweak_shell.py:162
#, python-format
msgid "%s theme updated successfully"
msgstr "Тема „%s“ је успешно ажурирана"

#: ../gtweak/tweaks/tweak_shell.py:164
#, python-format
msgid "%s theme installed successfully"
msgstr "Тема „%s“ је успешно инсталирана"

#: ../gtweak/tweaks/tweak_shell.py:172
msgid "Error installing theme"
msgstr "Не могу да инсталирам тему"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell.py:177
msgid "Invalid theme"
msgstr "Неисправна тема"

#: ../gtweak/tweaks/tweak_shell.py:195
msgid "Shell"
msgstr "Шкољка"

#: ../gtweak/tweaks/tweak_shell_extensions.py:35
msgid "Extension downloading"
msgstr "Преузимање проширења"

#: ../gtweak/tweaks/tweak_shell_extensions.py:37
msgid "Error loading extension"
msgstr "Не могу да учитам проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:39
msgid "Extension does not support shell version"
msgstr "Проширење не подржава ово издање шкољке"

#: ../gtweak/tweaks/tweak_shell_extensions.py:41
msgid "Unknown extension error"
msgstr "Непозната грешка у проширењу"

#: ../gtweak/tweaks/tweak_shell_extensions.py:46
#, python-format
msgid "%s Extension"
msgstr "%s проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:59
msgid "The shell must be restarted for changes to take effect"
msgstr "Морате да поново покренете шкољку како би применили измене"

#: ../gtweak/tweaks/tweak_shell_extensions.py:60
#: ../gtweak/tweaks/tweak_shell_extensions.py:125
msgid "Restart"
msgstr "Поново покрени"

#: ../gtweak/tweaks/tweak_shell_extensions.py:68
msgid "Install Shell Extension"
msgstr "Инсталирајте проширење шкољке"

#: ../gtweak/tweaks/tweak_shell_extensions.py:72
msgid "Select an extension"
msgstr "Изаберите проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:119
#, python-format
msgid "%s extension updated successfully"
msgstr "Проширење „%s“ је успешно ажурирано"

#: ../gtweak/tweaks/tweak_shell_extensions.py:121
#, python-format
msgid "%s extension installed successfully"
msgstr "Проширење „%s“ је успешно инсталирано"

#: ../gtweak/tweaks/tweak_shell_extensions.py:129
msgid "Error installing extension"
msgstr "Не могу да инсталирам проширење"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell_extensions.py:134
msgid "Invalid extension"
msgstr "Неисправно проширење"

#: ../gtweak/tweaks/tweak_shell_extensions.py:142
msgid "Shell Extensions"
msgstr "Проширења шкољке"

#: ../gtweak/tweaks/tweak_windows.py:31
msgid "Window theme"
msgstr "Тема прозора"

#. indicates the default theme, e.g Adwaita (default)
#: ../gtweak/utils.py:66
#, python-format
msgid "%s <i>(default)</i>"
msgstr "%s <i>(подразумевано)</i>"

#: ../gtweak/tweakmodel.py:26
msgid "Fonts"
msgstr "Писма"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "Тема"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "Радна површ"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
msgstr "Прозори"

#: ../gtweak/tweakmodel.py:37
msgid "Miscellaneous"
msgstr "Остало"

