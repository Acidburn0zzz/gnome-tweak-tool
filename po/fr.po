msgid "Fonts"
msgstr "Polices"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "Thème"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "Bureau"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
msgstr "Fenêtres"

