msgid "Could not list shell extensions"
msgstr "Tak bisa membuat daftar ekstensi shell"

#: ../gtweak/tweaks/tweak_shell.py:128
msgid "Select a theme"
msgstr "Pilih tema"

#: ../gtweak/tweaks/tweak_shell.py:179
#, python-format
msgid "%s theme updated successfully"
msgstr "Tema %s sukses dimutakhirkan"

#: ../gtweak/tweaks/tweak_shell.py:181
#, python-format
msgid "%s theme installed successfully"
msgstr "Tema %s sukses dipasang"

#: ../gtweak/tweaks/tweak_shell.py:189
msgid "Error installing theme"
msgstr "Galat saat memasang tema"

