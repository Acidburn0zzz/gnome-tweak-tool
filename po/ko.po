msgid "Fonts"
msgstr "글꼴"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "테마"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "데스크톱"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
