#: ../data/shell.ui.h:1
msgid "Tweak Tool"
msgstr "Nástroj Tweak"

#: ../data/shell.ui.h:2
msgid "tweaks"
msgstr "vyladění"

#: ../data/shell.ui.h:3
msgid "welcome"
msgstr "vítejte"

#: ../data/gnome-tweak-tool.desktop.in.h:1
msgid "Advanced Settings"
msgstr "Pokročilá nastavení"

#: ../data/gnome-tweak-tool.desktop.in.h:2
msgid "Tweak advanced GNOME 3 settings"
msgstr "Pokročilé vyladění nastavení GNOME 3"

#: ../gtweak/tweaks/tweak_font.py:27
msgid "Fonts"
msgstr "Písma"

#: ../gtweak/tweaks/tweak_interface.py:84 ../gtweak/tweaks/tweak_shell.py:106
msgid "<i>Default</i>"
msgstr "<i>Výchozí</i>"

#: ../gtweak/tweaks/tweak_interface.py:99
msgid "Interface"
msgstr "Rozhraní"

#: ../gtweak/tweaks/tweak_nautilus.py:48
msgid "File Manager"
msgstr "Správce souborů"

#. check the shell is running and the usertheme extension is present
#: ../gtweak/tweaks/tweak_shell.py:55
msgid "Unknown error"
msgstr "Neznámá chyba"

#: ../gtweak/tweaks/tweak_shell.py:59
msgid "Shell not running"
msgstr "Shell neběží"

#: ../gtweak/tweaks/tweak_shell.py:75
msgid "Shell user-theme extension incorrectly installed"
msgstr "Rozšíření s uživatelským motivem shellu je nesprávně nainstalováno"

#: ../gtweak/tweaks/tweak_shell.py:78
msgid "Shell user-theme extension not enabled"
msgstr "Rozšíření s uživatelským motivem shellu není povoleno"

#: ../gtweak/tweaks/tweak_shell.py:80
msgid "Could not list shell extensions"
msgstr "Nelze vypsat seznam rozšíření shellu"

#. a filechooser to install new themes
#: ../gtweak/tweaks/tweak_shell.py:111
msgid "Select a theme"
msgstr "Vybrat motiv"

#: ../gtweak/tweaks/tweak_shell.py:158
#, python-format
msgid "%s theme updated successfully"
msgstr "Motiv %s úspěšně aktualizován"

#: ../gtweak/tweaks/tweak_shell.py:160
#, python-format
msgid "%s theme installed successfully"
msgstr "Motiv %s úspěšně nainstalován"

#: ../gtweak/tweaks/tweak_shell.py:168
msgid "Error installing theme"
msgstr "Chyba instalace motivu"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell.py:173
msgid "Invalid theme"
msgstr "Neplatný motiv"

#: ../gtweak/tweaks/tweak_shell.py:194
msgid "The shell may need to be restarted to apply the theme"
msgstr "Shell možná bude zapotřebí restartovat, aby se motiv použil"

#: ../gtweak/tweaks/tweak_shell.py:195
#: ../gtweak/tweaks/tweak_shell_extensions.py:62
#: ../gtweak/tweaks/tweak_shell_extensions.py:127
msgid "Restart"
msgstr "Restartovat"

#: ../gtweak/tweaks/tweak_shell.py:202
msgid "Shell"
msgstr "Shell"

#: ../gtweak/tweaks/tweak_shell_extensions.py:40
msgid "Error loading extension"
msgstr "Chyba načítání rozšíření"

#: ../gtweak/tweaks/tweak_shell_extensions.py:42
msgid "Extension does not support shell version"
msgstr "Rozšíření nepodporuje tuto verzi shellu"

#: ../gtweak/tweaks/tweak_shell_extensions.py:44
msgid "Unknown extension error"
msgstr "Neznámá chyba rozšíření"

#: ../gtweak/tweaks/tweak_shell_extensions.py:49
#, python-format
msgid "%s Extension"
msgstr "Rozšíření %s"

#: ../gtweak/tweaks/tweak_shell_extensions.py:61
msgid "The shell must be restarted for changes to take effect"
msgstr "Shell musí být restartován, aby se změny projevily"

#: ../gtweak/tweaks/tweak_shell_extensions.py:70
msgid "Install Shell Extension"
msgstr "Instalovat rozšíření shellu"

#: ../gtweak/tweaks/tweak_shell_extensions.py:74
msgid "Select an extension"
msgstr "Vybrat rozšíření"

#: ../gtweak/tweaks/tweak_shell_extensions.py:121
#, python-format
msgid "%s extension updated successfully"
msgstr "Rozšíření %s úspěšně aktualizováno"

#: ../gtweak/tweaks/tweak_shell_extensions.py:123
#, python-format
msgid "%s extension installed successfully"
msgstr "Rozšíření %s úspěšně nainstalováno"

#: ../gtweak/tweaks/tweak_shell_extensions.py:131
msgid "Error installing extension"
msgstr "Chyba instalace rozšíření"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell_extensions.py:136
msgid "Invalid extension"
msgstr "Neplatné rozšíření"

#: ../gtweak/tweaks/tweak_shell_extensions.py:169
msgid "Shell Extensions"
msgstr "Rozšíření Shellu"

#: ../gtweak/tweaks/tweak_windows.py:78
msgid "Windows"
msgstr "Okna"

#. indicates the default theme, e.g Adwaita (default)
#: ../gtweak/utils.py:53
#, python-format
msgid "%s <i>(default)</i>"
msgstr "%s <i>(výchozí)</i>"
