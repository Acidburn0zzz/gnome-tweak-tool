msgid "Tweak advanced GNOME 3 settings"
msgstr "Изменение дополнительных параметров GNOME 3"

#: ../data/shell.ui.h:2
msgid "welcome"
msgstr "добро пожаловать"

#: ../data/shell.ui.h:3
msgid "tweaks"
msgstr "настройки"

#: ../gtweak/tweakmodel.py:28
msgid "Fonts"
msgstr "Шрифты"

