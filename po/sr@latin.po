msgid "Fonts"
msgstr "Fontovi"

#: ../gtweak/tweakmodel.py:27
msgid "Theme"
msgstr "Tema"

#: ../gtweak/tweakmodel.py:28
msgid "Desktop"
msgstr "Radna površ"

#: ../gtweak/tweakmodel.py:29
msgid "Windows"
msgstr "Prozori"

