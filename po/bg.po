msgid "Shell Extensions"
msgstr "Разширения на обвивката"

#: ../gtweak/tweaks/tweak_shell.py:44
msgid "Close Only"
msgstr "Само затваряне"

#: ../gtweak/tweaks/tweak_shell.py:45
msgid "Minimize and Close"
msgstr "Минимизиране и затваряне"

#: ../gtweak/tweaks/tweak_shell.py:46
msgid "Maximize and Close"
msgstr "Максимизиране и затваряне"

#: ../gtweak/tweaks/tweak_shell.py:47
msgid "All"
msgstr "Всички"

#: ../gtweak/tweaks/tweak_shell.py:61
msgid "Shell theme"
msgstr "Тема на обвивката"

#: ../gtweak/tweaks/tweak_shell.py:61
msgid "Install custom or user themes for gnome-shell"
msgstr "Инсталиране на друга тема за обвивката на GNOME"

#. check the shell is running and the usertheme extension is present
#: ../gtweak/tweaks/tweak_shell.py:64
msgid "Unknown error"
msgstr "Непозната грешка"

#: ../gtweak/tweaks/tweak_shell.py:72
msgid "Shell not running"
msgstr "Обвивката не е стартирана"

#: ../gtweak/tweaks/tweak_shell.py:94
msgid "Shell user-theme extension incorrectly installed"
msgstr "Разширението за теми на обвивката на GNOME е инсталирано неправилно"

#: ../gtweak/tweaks/tweak_shell.py:97
msgid "Shell user-theme extension not enabled"
msgstr "Разширението за теми на обвивката на GNOME не е включено"

#: ../gtweak/tweaks/tweak_shell.py:100
msgid "Could not list shell extensions"
msgstr "Разширенията на обвивката на GNOME не могат да бъдат изброени"

#: ../gtweak/tweaks/tweak_shell.py:133
msgid "Select a theme"
msgstr "Избор на тема"

#: ../gtweak/tweaks/tweak_shell.py:184
#, python-format
msgid "%s theme updated successfully"
msgstr "Темата „%s“ е успешно обновена"

#: ../gtweak/tweaks/tweak_shell.py:186
#, python-format
msgid "%s theme installed successfully"
msgstr "Темата „%s“ е успешно инсталирана"

#: ../gtweak/tweaks/tweak_shell.py:194
msgid "Error installing theme"
msgstr "Грешка при инсталиране на тема"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell.py:199
msgid "Invalid theme"
msgstr "Неправилна тема"

#: ../gtweak/tweaks/tweak_shell.py:223
msgid "Dynamic workspaces"
msgstr "Динамични работни места"

#: ../gtweak/tweaks/tweak_shell.py:223
msgid "Disable gnome-shell dynamic workspace management, use static workspaces"
msgstr ""
"Изключване на динамичното управление на работни места в обвивката на GNOME. "
"Да се използват статичен брой места."

#: ../gtweak/tweaks/tweak_typing.py:42 ../gtweak/tweaks/tweak_windows.py:51
msgid "Disabled"
msgstr "Изключени"

#. indicates the default theme, e.g Adwaita (default)
#: ../gtweak/utils.py:67
#, python-format
msgid "%s <i>(default)</i>"
msgstr "„%s“ <i>стандартно</i>"

#: ../gtweak/widgets.py:345
msgid "Enable dark theme for all applications"
msgstr "Включване на тъмна тема за всички приложения"

#: ../gtweak/widgets.py:346
msgid "Enable the dark theme hint for all the applications in the session"
msgstr "Включване на подсказка за тъмна тема за всички програми в сесията"

#: ../gtweak/widgets.py:364
msgid "Error writing setting"
msgstr "Грешка при запазването на настройките"
