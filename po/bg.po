msgid "<i>Default</i>"
msgstr "<i>Стандартно</i>"

#: ../gtweak/tweaks/tweak_shell_extensions.py:38
msgid "Extension downloading"
msgstr "Изтегляне на разширение"

#: ../gtweak/tweaks/tweak_shell_extensions.py:40
msgid "Error loading extension"
msgstr "Грешка при изтеглянето на разширение"

#: ../gtweak/tweaks/tweak_shell_extensions.py:42
msgid "Extension does not support shell version"
msgstr "Разширението не поддържа тази версия на обвивката"

#: ../gtweak/tweaks/tweak_shell_extensions.py:44
msgid "Unknown extension error"
msgstr "Непозната грешка от разширението"

#: ../gtweak/tweaks/tweak_shell_extensions.py:75
msgid "The shell must be restarted for changes to take effect"
msgstr "Промените ще влязат в сила след рестартирането на обвивката"

#: ../gtweak/tweaks/tweak_shell_extensions.py:76
#: ../gtweak/tweaks/tweak_shell_extensions.py:148
msgid "Restart"
msgstr "Рестартиране"

#: ../gtweak/tweaks/tweak_shell_extensions.py:82
msgid "Install Shell Extension"
msgstr "Инсталиране на разширение на обвивката"

#: ../gtweak/tweaks/tweak_shell_extensions.py:87
msgid "Select an extension"
msgstr "Избор на разширение"

#: ../gtweak/tweaks/tweak_shell_extensions.py:94
msgid "Get more extensions"
msgstr "Изтегляне на още разширения"

#: ../gtweak/tweaks/tweak_shell_extensions.py:142
#, python-format
msgid "%s extension updated successfully"
msgstr "Успешно обновяване на разширението „%s“"

#: ../gtweak/tweaks/tweak_shell_extensions.py:144
#, python-format
msgid "%s extension installed successfully"
msgstr "Успешна инсталация на разширението „%s“"

#: ../gtweak/tweaks/tweak_shell_extensions.py:152
msgid "Error installing extension"
msgstr "Грешка при инсталирането на разширение"

