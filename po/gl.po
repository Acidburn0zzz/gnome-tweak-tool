msgid "The shell must be restarted for changes to take effect"
msgstr "Débese reiniciar GNOME-Shell para que se apliquen os cambios"

#: ../gtweak/tweaks/tweak_shell_extensions.py:74
#: ../gtweak/tweaks/tweak_shell_extensions.py:143
msgid "Restart"
msgstr "Reiniciar"

#: ../gtweak/tweaks/tweak_shell_extensions.py:80
msgid "Install Shell Extension"
msgstr "Instalar extensión de GNOME-Shell"

#: ../gtweak/tweaks/tweak_shell_extensions.py:84
msgid "Select an extension"
msgstr "Seleccionar unha extensión"

#: ../gtweak/tweaks/tweak_shell_extensions.py:89
msgid "Get more extensions"
msgstr "Obter máis extensións"

#: ../gtweak/tweaks/tweak_shell_extensions.py:137
#, python-format
msgid "%s extension updated successfully"
msgstr "Extensión %s anovada correctamente"

#: ../gtweak/tweaks/tweak_shell_extensions.py:139
#, python-format
msgid "%s extension installed successfully"
msgstr "Extensión %s instalada correctamente"

#: ../gtweak/tweaks/tweak_shell_extensions.py:147
msgid "Error installing extension"
msgstr "Produciuse un erro ao instalar a extensión"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell_extensions.py:152
msgid "Invalid extension"
msgstr "Extensión non válida"

#: ../gtweak/tweaks/tweak_shell_extensions.py:160
msgid "Shell Extensions"
msgstr "Extensións de GNOME-Shell"

#. check the shell is running and the usertheme extension is present
#: ../gtweak/tweaks/tweak_shell.py:58
msgid "Unknown error"
msgstr "Erro descoñecido"

#: ../gtweak/tweaks/tweak_shell.py:63
msgid "Shell not running"
msgstr "Non se está executando GNOME-Shell"

#: ../gtweak/tweaks/tweak_shell.py:84
msgid "Shell user-theme extension incorrectly installed"
msgstr ""
"Extensión do tema de usuario de GNOME-Shell non instalado correctamente"

#: ../gtweak/tweaks/tweak_shell.py:87
msgid "Shell user-theme extension not enabled"
msgstr "Extensión do tema de usuario de GNOME-Shell non activada"

#: ../gtweak/tweaks/tweak_shell.py:90
msgid "Could not list shell extensions"
msgstr "Non foi posíbel listar as extensión de GNOME-Shell"

#. a filechooser to install new themes
#: ../gtweak/tweaks/tweak_shell.py:122
msgid "Select a theme"
msgstr "Seleccionar un tema"

#: ../gtweak/tweaks/tweak_shell.py:171
#, python-format
msgid "%s theme updated successfully"
msgstr "Tema %s actualizado correctamente"

#: ../gtweak/tweaks/tweak_shell.py:173
#, python-format
msgid "%s theme installed successfully"
msgstr "Tema %s instalado correctamente"

#: ../gtweak/tweaks/tweak_shell.py:181
msgid "Error installing theme"
msgstr "Produciuse un erro ao instalar o tema"

#. does not look like a valid theme
#: ../gtweak/tweaks/tweak_shell.py:186
msgid "Invalid theme"
msgstr "Tema non válido"

#: ../gtweak/tweaks/tweak_windows.py:31
msgid "Window theme"
msgstr "Tema da xanela"

#. indicates the default theme, e.g Adwaita (default)
#: ../gtweak/utils.py:67
#, python-format
msgid "%s <i>(default)</i>"
msgstr "<i>(predeterminado)</i> %s"

#: ../gtweak/widgets.py:305
msgid "Enable dark theme for all applications"
msgstr "Activar o tema escuro para todos os aplicativos"

#: ../gtweak/widgets.py:306
msgid "Enable the dark theme hint for all the applications in the session"
msgstr "Activar a suxestión de tema escuro para todos os aplicativos na sesión"

#: ../gtweak/widgets.py:324
msgid "Error writing setting"
msgstr "Produciuse un erro ao gravar a configuración"

